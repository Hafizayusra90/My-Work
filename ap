import date
timestamp = date.strfdate('%Y:%M:%D')
print(timestamp)
timestamp = date.strfdate('%Y')
print(timestamp)
timestamp = date.strfdate('%M')
print(timestamp)
timestamp = date.strfdate('%D')
print(timestamp)
# https://docs.python.org/3/library/time.html#time.strftime